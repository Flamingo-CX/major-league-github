{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "npm",
            "script": "dev",
            "path": "frontend",
            "isBackground": true,
            "problemMatcher": [
                {
                    "owner": "webpack",
                    "pattern": [
                        {
                            "regexp": "ERROR in (.*?)(?: \\((.*?)\\))?:(.*)$",
                            "file": 1,
                            "line": 2,
                            "message": 3
                        }
                    ],
                    "background": {
                        "activeOnStart": true,
                        "beginsPattern": {
                            "regexp": "(webpack.*)|(node:.*)"
                        },
                        "endsPattern": {
                            "regexp": "webpack [0-9.]+ compiled successfully"
                        }
                    }
                }
            ],
            "group": "build",
            "label": "npm: dev",
            "detail": "webpack serve",
            "presentation": {
                "reveal": "silent",
                "panel": "dedicated"
            }
        }
    ]
} 